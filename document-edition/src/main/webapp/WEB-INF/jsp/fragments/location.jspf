<c:if test="${form.creation}">
    <div class="form-group">
        <label><op:translate key="DOCUMENT_EDITION_FORM_LOCATION_LABEL"/></label>
        <ol class="breadcrumb bg-light">
            <c:forEach var="item" items="${form.breadcrumb}">
