<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE portlet-app PUBLIC "-//JBoss Portal//DTD JBoss Portlet 2.6//EN" "http://www.jboss.org/portal/dtd/jboss-portlet_2_6.dtd">

<portlet-app>

    <portlet>
        <portlet-name>osivia-services-taskbar</portlet-name>

        <transaction>
            <trans-attribute>Required</trans-attribute>
        </transaction>

        <header-content>
            <link rel="stylesheet" href="/css/taskbar.min.css" />
            
            <script type="text/javascript" src="/js/taskbar.js"></script>
        </header-content>

        <portlet-info>
            <icon>
                <small-icon>/img/portlet_icon.png</small-icon>
                <large-icon>/img/portlet_icon.png</large-icon>
            </icon>
        </portlet-info>
    </portlet>
    
</portlet-app>
